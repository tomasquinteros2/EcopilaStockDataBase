<!DOCTYPE symmetric-config PUBLIC "-//OpenSymphony Group//DTD SymmetricDS 3.0//EN" "http://www.symmetricds.org/dtd/symmetric-3.0.dtd">
<symmetric-config>

    <trigger id="producto_trigger" expression="producto" sync_on_insert="true" sync_on_update="true" sync_on_delete="true"/>
    <trigger id="proveedor_trigger" expression="proveedor" sync_on_insert="true" sync_on_update="true" sync_on_delete="true"/>
    <trigger id="tipo_producto_trigger" expression="tipo_producto" sync_on_insert="true" sync_on_update="true" sync_on_delete="true"/>
    <trigger id="dolar_trigger" expression="dolar" sync_on_insert="true" sync_on_update="true" sync_on_delete="true"/>
    <trigger id="usuario_trigger" expression="usuario" sync_on_insert="true" sync_on_update="true" sync_on_delete="true"/>
    <trigger id="authority_trigger" expression="authority" sync_on_insert="true" sync_on_update="true" sync_on_delete="true"/>

    <router id="default_router" target_node_group_id="client_group"/>

    <channel id="producto_channel"/>
    <channel id="proveedor_channel"/>
    <channel id="tipo_producto_channel"/>
    <channel id="dolar_channel"/>
    <channel id="auth_channel"/>

    <router id="master_to_client" target_node_group_id="client_group"/>

    <trigger-router trigger_id="producto_trigger" router_id="master_to_client" channel_id="producto_channel"/>
    <trigger-router trigger_id="proveedor_trigger" router_id="master_to_client" channel_id="proveedor_channel"/>
    <trigger-router trigger_id="tipo_producto_trigger" router_id="master_to_client" channel_id="tipo_producto_channel"/>
    <trigger-router trigger_id="dolar_trigger" router_id="master_to_client" channel_id="dolar_channel"/>
    <trigger-router trigger_id="usuario_trigger" router_id="master_to_client" channel_id="auth_channel"/>
    <trigger-router trigger_id="authority_trigger" router_id="master_to_client" channel_id="auth_channel"/>

    <node-group id="master_group" description="Master Node Group"/>
    <node-group id="client_group" description="Client Node Group"/>

    <node-group-link source_node_group_id="master_group" target_node_group_id="client_group" data_event_action="P"/>
    <node-group-link source_node_group_id="client_group" target_node_group_id="master_group" data_event_action="P"/>

</symmetric-config>